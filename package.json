{
  "name": "redefy",
  "version": "1.0.0-alpha.0",
  "description": "react components for myself",
  "keywords": [
    "react",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codpoe/redefy.git"
  },
  "author": "codpoe <codpoe.me@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "src",
    "lib",
    "es"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "sh ./scripts/dev.sh",
    "clean": "rm -rf es lib",
    "build": "npm run clean && npm run build:es && npm run build:lib && npm run build:style",
    "build:es": "tsc",
    "build:lib": "tsc --module CommonJS --outDir lib",
    "build:style": "postcss src/index.css src/**/*.css --dir es --base src && node ./scripts/copy-styles.js",
    "build:icons": "node scripts/build-icons.js",
    "build:doc": "docz build",
    "start:doc": "docz build && docz serve",
    "test": "jest --coverage",
    "release": "release-it",
    "release:beta": "release-it --preRelease=beta"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react-transition-group": "^4.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@toolman/icon-builder": "^3.0.0",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.2",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "cssnano": "^4.1.10",
    "docz": "^2.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "fs-extra": "^8.1.0",
    "gatsby-plugin-postcss": "^2.1.19",
    "jest": "^24.7.1",
    "ora": "^3.4.0",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.8.6",
    "release-it": "^12.4.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
